<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sydney.pos">

    <uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:label="ADMIN"
            android:name=".LoginAdmin" >
        </activity>

        <activity
            android:label="CASHIER"
            android:name=".LoginCashier" >
        </activity>

        <activity
            android:label="REGISTRATION"
            android:name=".Registration" >
        </activity>

        <activity
            android:label="CASHIERUI"
            android:name=".CashierUI" >
        </activity>

        <activity
            android:label="ADMINUI"
            android:name=".AdminUI" >
        </activity>

        <activity
            android:label="ADDITEM"
            android:name=".AddItem" >
        </activity>
        <activity android:name=".Departments"
                  android:label="departments">
        </activity>


    </application>

</manifest>